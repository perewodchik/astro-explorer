{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
        "root/*": ["./*"],
        "src/*": ["./src/*"],
        "@app/*": ["./src/app/*"],
        "@shared/*": ["./src/shared/*"],
        "@entities/*": ["./src/entities/*"],
        "@features/*": ["./src/features/*"],
        "@pages/*": ["./src/pages/*"]
    }
  }
}
